{
  "name": "electron_first_app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/main/index.js",
  "scripts": {
    "copy:resources": "cpx \"prod_resources/**/*\" dist",
    "copy:photon": "cpx \"node_modules/photon/dist/css/**/*\" dist/css && cpx \"node_modules/photon/dist/fonts/**/*\" dist/fonts",
    "clean": "rimraf .tmp dist release",
    "test": "eslint src/**/*.js src/**/*.jsx",
    "prestart": "npm run copy:resources && npm run copy:photon && webpack",
    "start": "electron .",
    "package": "npm run prestart && electron-packager dist \"Electron_Test\" --overwrite --platform=darwin --out=release --icon=\"icon/icon\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.23.1",
    "electron": "^1.6.7",
    "electron-packager": "^8.5.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "photon": "github:connors/photon",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0"
  }
}
